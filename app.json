{
  "expo": {
    "name": "ReactNativeTutorial",
    "slug": "ReactNativeTutorial",
    "version": "1.0.0",
    "orientation": "portrait", // can set to landscape or default which supports both orientations
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
